    pageEncoding="UTF-8"%>
    <%@ taglib uri="http://www.springframework.org/security/tags" prefix="sec" %>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	welcome:member
	
	<c:if test="${not empty pageContext.request.userPrincipal }">
	<p>is Log-In</p>
	</c:if>
	
	<c:if tes"${not empty pageContext.request.userPrincipal }">
	<p>if Log-Out</p>
	</c:if>
</body>
